# django-humans-txt
# .coveragerc


[run]
branch = True
source = humans_txt
omit =
    */__init__.py
   humans_txt/apps.py
   humans_txt/migrations/*.py
   humans_txt/conf.py
   humans_txt/urls.py
   humans_txt/admin/*.py

[report]
omit =
    */__init__.py
    humans_txt/apps.py
    humans_txt/migrations/*.py
    humans_txt/conf.py
    humans_txt/urls.py
    humans_txt/admin/*.py
exclude_lines =
    pragma: no cover

